#include <iostream>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t--)
	{
	    int num;
	    cin >> num;
	    int c = 0;
	    while(num>0){
	       int re = num%10;
	       if(re==7)
	       {
	           c++;
	       }
	       num  = num/10;
	    }
	    if(c!=0)
	    {
	        cout << "YES" << endl;
	    }
	    else{
	        cout <<"NO" << endl;
	    }
	}
	return 0;
}
